package com.montydigital.CopsAndRobbers.ui.gamePlay{
	import com.montydigital.CopsAndRobbers.ui.UIAssets;
	
	import starling.display.StImage;
	import starling.display.StSprite;
	
	
	[Event(name="play", type="flash.events.Event")]
	[Event(name="restart", type="flash.events.Event")]
	[Event(name="options", type="flash.events.Event")]
	public class PauseScreen extends StSprite{
		public function PauseScreen(){
			super();
			this.addChild(StImage.fromBitmap(new UIAssets.PlayBtn()));
		}
	}
}